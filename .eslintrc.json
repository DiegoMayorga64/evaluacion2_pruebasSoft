{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "implicit-arrow-linebreak": "off",
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
    }
}
